<form method="delete" action="{{ path('app_project_delete', {'id': project.id}) }}" onsubmit="return confirm('{{ 'Are you sure you want to delete this item?'|trans }}');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ project.id) }}">
    <button class="btn btn-danger">
        {{ ux_icon('tabler:trash') }}
        {{ 'Delete'|trans }}
    </button>
</form>
